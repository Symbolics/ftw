;; special case for handles which need to be pointers 
(defmacro define-constant-hwnd (name value &optional doc)
  `(alexandria:define-constant ,name (make-pointer ,value)
     :test #'pointer-eq
     ,@(when doc `(:documentation ,doc))))

(defmacro define-constant-string (name value &optional doc)
  `(alexandria:define-constant ,name ,value
     :test #'string=
     ,@(when doc `(:documentation ,doc))))


